{
  "framework": "nextjs",
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" },
        { "key": "X-Frame-Options", "value": "DENY" },
        { "key": "X-XSS-Protection", "value": "1; mode=block" }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/#/privacy",
      "destination": "/privacy",
      "permanent": true
    },
    {
      "source": "/#/terms",
      "destination": "/terms",
      "permanent": true
    }
  ]
}
